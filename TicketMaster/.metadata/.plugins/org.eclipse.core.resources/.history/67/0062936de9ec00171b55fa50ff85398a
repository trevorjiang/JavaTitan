	private String urlEncodeHelper(String term) {
		try {
			term = java.net.URLEncoder.encode(term, "UTF-8");
		} catch (Exception e) {
			e.printStackTrace();
		}
		return term;
	}
	private void queryAPI(double lat, double lon) {
		JSONArray events = search(lat, lon, null);
